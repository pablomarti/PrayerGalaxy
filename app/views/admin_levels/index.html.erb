<h1>Listing admin_levels</h1>

<table>
  <tr>
    <th>Level</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_levels.each do |admin_level| %>
  <tr>
    <td><%= admin_level.level %></td>
    <td><%= link_to 'Show', admin_level %></td>
    <td><%= link_to 'Edit', edit_admin_level_path(admin_level) %></td>
    <td><%= link_to 'Destroy', admin_level, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

